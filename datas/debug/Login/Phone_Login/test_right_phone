  - desc: '正确的密码'
    http:
      path: '/api/user/login'
      headers: {'Content-Type': 'application/json'}
      body: {platform: 4, token: login, id_token: '+8618515588536', secret: '111111'}
    expected:
      code: 200
      err_msg: 'success'
